{
  "name": "example",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@pencel/cli": "workspace:*",
    "@types/bun": "latest",
    "ts-flattered": "link:ts-flattered",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "scripts": {
    "generate": "bun ../packages/cli/src/main.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "nx": {
    "sourceRoot": "example",
    "projectType": "application",
    "targets": {
      "run": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "bun run generate",
          "cwd": "example"
        }
      }
    }
  },
  "dependencies": {
    "@stencil/core": "^4.37.1",
    "vite": "^7.1.2"
  }
}
