{
  "name": "@pencel/cli",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "pencel": "dist/main.mjs"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@pencel/core": "workspace:*",
    "c12": "^2.0.1",
    "clipanion": "^4.0.0-rc.4"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "^1.2.21"
  },
  "nx": {
    "sourceRoot": "packages/cli",
    "projectType": "application",
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "tsdown",
          "cwd": "packages/cli"
        }
      },
      "run": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "bun src/main.ts",
          "cwd": "packages/cli"
        }
      }
    }
  }
}
