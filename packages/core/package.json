{
  "name": "@pencel/core",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "map": "./dist/index.js.map"
    }
  },
  "nx": {
    "sourceRoot": "packages/core",
    "projectType": "library",
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "tsdown",
          "cwd": "packages/core"
        }
      }
    }
  }
}
