{
  "name": "@pencil/core",
  "version": "0.0.0",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "nx": {
    "sourceRoot": "packages/core",
    "projectType": "library",
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "bun build.ts",
          "cwd": "packages/core"
        }
      }
    }
  }
}
