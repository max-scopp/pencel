{
  "name": "@pencil/runtime-examples",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nx serve",
    "build": "nx build"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@pencil/runtime": "^0.0.1"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "nx": {
    "sourceRoot": "packages/runtime/examples",
    "projectType": "application",
    "targets": {
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite",
          "cwd": "packages/runtime/examples"
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite build",
          "cwd": "packages/runtime/examples"
        }
      }
    }
  }
}
