{
  "name": "@pencel/runtime-examples",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@pencel/runtime": "^0.0.1",
    "@pencel/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "npm:rolldown-vite@latest"
  },
  "nx": {
    "sourceRoot": "packages/runtime/examples",
    "projectType": "application",
    "targets": {
      "serve": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "vite",
          "cwd": "packages/runtime/examples"
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite build",
          "cwd": "packages/runtime/examples"
        }
      }
    }
  }
}
