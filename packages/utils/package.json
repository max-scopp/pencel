{
  "name": "@pencil/utils",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "nx": {
    "sourceRoot": "packages/utils",
    "projectType": "library",
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "bun build.ts",
          "cwd": "packages/utils"
        }
      }
    }
  }
}
